		<property name="dataAuth" type="java.lang.String">
			<column name="DATA_AUTH " length="40" />
		</property>
		
		<property name="reszipfilename" type="java.lang.String">
			<column name="RESZIPFILENAME" length="300" />
		</property> 
		
	</class>
</hibernate-mapping>