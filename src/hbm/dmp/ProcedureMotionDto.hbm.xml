<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.more.fw.buss.motion.dmp.model.ProcedureMotionDto" table="T_OPERATION_ACTION">
		
		<id name="id" type="java.lang.String">
			<column name="ID" length="50" />
		</id>
		<property name="toaCode" type="java.lang.String">
			<column name="TOA_CODE" length="50" />
		</property>
		<property name="toaName" type="java.lang.String">
			<column name="TOA_NAME" length="200" />
		</property>
		<property name="toaMsgCode" type="java.lang.String">
			<column name="TOA_MSG_CODE" length="100" />
		</property>
		<property name="toaParamCode" type="java.lang.String">
			<column name="TOA_PARAM_CODE" length="100" />
		</property>	
		<property name="toaParamLen" type="java.lang.String">
			<column name="TOA_PARAM_LEN" length="100" />
		</property>
		<property name="toaType" type="java.lang.String">
			<column name="TOA_TYPE" length="100" />
		</property>
		<property name="toaRemark" type="java.lang.String">
			<column name="TOA_REMARK" length="4000" />
		</property>
		<property name="toaClassify" type="java.lang.String">
			<column name="TOA_CLASSIFY" length="50" />
		</property>
	</class>
</hibernate-mapping>