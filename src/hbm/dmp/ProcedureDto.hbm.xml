<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.more.fw.buss.dmp.model.ProcedureDto" table="T_PROCEDURE_LIST">
		<id name="id" type="java.lang.String">
			<column name="ID" length="50" />
		</id>
		<property name="wplCode" type="java.lang.String">
			<column name="WPL_CODE" length="50" />
		</property>
		<property name="wplName" type="java.lang.String">
			<column name="WPL_NAME" length="200" />
		</property>
		<property name="wplType" type="java.lang.String">
			<column name="WPL_TYPE" length="100" />
		</property>
		<property name="wplDesc" type="java.lang.String">
			<column name="WPL_DESC" length="4000" />
		</property>
	
	
	</class>
</hibernate-mapping>