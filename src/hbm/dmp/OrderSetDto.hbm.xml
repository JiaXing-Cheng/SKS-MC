<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.more.fw.buss.dmp.model.OrderSetDto" table="T_COMMAND_SET">
		<id name="tcsCode" type="java.lang.String">
			<column name="TCS_CODE" length="50" />
		</id>
		<property name="tcsName" type="java.lang.String">
			<column name="TCS_NAME" length="100" />
		</property>
		<property name="tcsRemark" type="java.lang.String">
			<column name="TCS_REMARK" length="100" />
		</property>
	</class>
</hibernate-mapping>