<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.more.fw.inf.common.model.TemplateDto" table="MS_SY_INF_TEMPLATE" dynamic-update="true">
		<id name="id" type="java.lang.String">
			<column name="ID" length="33" />
		</id>
		<property name="deptId" type="java.lang.String">
			<column name="DEPT_ID" length="33" />
		</property>
		<property name="createUser" type="java.lang.String">
			<column name="CREATE_USER" length="33" />
		</property>
		<property name="createTime" type="java.util.Date">
			<column name="CREATE_TIME" length="30" />
		</property>
		<property name="editUser" type="java.lang.String">
			<column name="EDIT_USER" length="33" />
		</property>
		<property name="editTime" type="java.util.Date">
			<column name="EDIT_TIME" length="1" />
		</property>
		<property name="title" type="java.lang.String">
			<column name="TITLE" length="128" />
		</property>
		<property name="code" type="java.lang.String">
			<column name="CODE" length="128" />
		</property>
		<property name="isDefault" type="java.lang.String">
			<column name="IS_DEFAULT" length="2" />
		</property>
		<property name="template" type="java.lang.String">
			<column name="TEMPLATE" length="3000" />
		</property>
		<property name="example" type="java.lang.String">
			<column name="EXAMPLE" length="3000" />
		</property>
		<property name="wxTemplateId" type="java.lang.String">
			<column name="WX_TEMPLATE_ID" length="33" />
		</property>
		<property name="isSystem" type="java.lang.String">
			<column name="IS_SYSTEM" length="2" />
		</property>
	
	</class>
</hibernate-mapping>