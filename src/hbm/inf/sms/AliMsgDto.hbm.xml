<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.more.fw.inf.sms.model.AliMsgDto"
		table="ms_ali_mesg">
		<id name="id" type="java.lang.String">
			<column name="ID" length="33" />
		</id>
		<property name="deptId" type="java.lang.String">
			<column name="DEPT_ID" length="33" />
		</property>
		<property name="createUser" type="java.lang.String">
			<column name="CREATE_USER" length="33" />
		</property>
		<property name="createTime" type="java.util.Date">
			<column name="CREATE_TIME" length="30" />
		</property>
		<property name="editUser" type="java.lang.String">
			<column name="EDIT_USER" length="33" />
		</property>
		<property name="editTime" type="java.util.Date">
			<column name="EDIT_TIME" length="1" />
		</property>
		<property name="name" type="java.lang.String">
			<column name="NAME" length="255" />
		</property>
		<property name="accessKeyId" type="java.lang.String">
			<column name="ACCESS_KEY_SECRET" length="255" />
		</property>
		<property name="accessKeySecrt" type="java.lang.String">
			<column name="ACCESS_KEY_ID" length="255" />
		</property>
		<property name="autograph" type="java.lang.String">
			<column name="AUTOGRAPH" length="4000" />
		</property>
		<property name="type" type="java.lang.String">
			<column name="TYPE" length="10" />
		</property>
		<property name="memo" type="java.lang.String">
			<column name="MEMO" length="4000"/>
		</property>
	</class>
</hibernate-mapping>