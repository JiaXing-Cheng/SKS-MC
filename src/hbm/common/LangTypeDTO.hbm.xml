<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.more.fw.core.dbo.model.dto.LangTypeDto" table="MS_LANGUAGE_TYPE">
		<id name="id" type="java.lang.String">
			<column name="id" length="33"></column>
		</id>
		<property name="languageCode" type="java.lang.String">
			<column name="language_code" length="33"></column>
		</property>
		<property name="languageName" type="java.lang.String">
			<column name="language_name" length="100"></column>
		</property>
		<property name="languageShowName" type="java.lang.String">
			<column name="language_show_name" length="100"></column>
		</property>
		<property name="languageCounter" type="java.lang.String">
			<column name="language_counter" length="100"></column>
		</property>
		<property name="languageOrder" type="java.lang.String">
			<column name="language_order" length="10"></column>
		</property>
		<property name="languageEnName" type="java.lang.String">
			<column name="language_en_name" length="100"></column>
		</property>
		<property name="languageKey" type="java.lang.String">
			<column name="language_key" length="100"></column>
		</property>
		<property name="createDate" type="java.util.Date">
			<column name="create_date"></column>
		</property>
	</class>
</hibernate-mapping>