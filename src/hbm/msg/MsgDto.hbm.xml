<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.more.fw.core.sysmain.msg.model.MsgDto" table="SY_MSG">
		<id name="id" type="java.lang.String">
			<column name="ID" length="33" />
		</id>
		<property name="deptId" type="java.lang.String">
			<column name="DEPT_ID" length="33" />
		</property>
		<property name="createUser" type="java.lang.String">
			<column name="CREATE_USER" length="33" />
		</property>
		<property name="createTime" type="java.util.Date">
			<column name="CREATE_TIME" />
		</property>
		<property name="editUser" type="java.lang.String">
			<column name="EDIT_USER" length="33" />
		</property>

		<property name="editTime" type="java.util.Date">
			<column name="EDIT_TIME" />
		</property>
		<property name="type" type="java.lang.String">
			<column name="type" length="3" />
		</property>
		<property name="title" type="java.lang.String">
			<column name="TITLE" length="300" />
		</property>
		<property name="text" type="java.lang.String">
			<column name="text" length="3000" />
		</property>
		<property name="seq" type="java.lang.Long">
			<column name="seq" length="15" />
		</property>
		<!-- <property name="recObjType" type="java.lang.String">
			<column name="REC_OBJ_TYPE" length="1" />
		</property> 
		<property name="recObj" type="java.lang.String">
			<column name="REC_OBJ" length="4000" />
		</property>-->
		<property name="DATA_AUTH" type="java.lang.String">
			<column name="DATA_AUTH" length="40" />
		</property>
		<property name="userId" type="java.lang.String">
			<column name="USERID" length="33" />
		</property>
		<property name="status" type="java.lang.String">
			<column name="STATUS" length="1" />
		</property>
		<property name="historyid" type="java.lang.String">
			<column name="HISTORYID" length="200" />
		</property>
	</class>
</hibernate-mapping>