<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.more.fw.core.sysfa.usermessage.model.AdminTargetPicDto" table="SY_ADMIN_TARGET_PIC">
		<id name="id" type="java.lang.String">
			<column name="ID" length="33" />
		</id>
		<property name="createId" type="java.lang.String">
			<column name="CREATE_ID" length="33" />
		</property>
		<property name="createDate" type="java.util.Date">
			<column name="CREATE_DATE"  />
		</property>
		<property name="picName" type="java.lang.String">
			<column name="PIC_NAME" length="300" />
		</property>
		<property name="photo" type="java.sql.Blob">
			<column name="PHOTO"  />
		</property>
	</class>
</hibernate-mapping>