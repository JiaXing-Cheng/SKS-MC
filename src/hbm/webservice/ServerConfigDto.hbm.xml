		<property name="configDeclare" type="java.lang.String" >
			<column name="SERVICE_CONFIG_DECLARE" length="4000"/>
		</property>
		<property name="moduleId" type="java.lang.String">
			<column name="MODEL_ID" length="33" />
		</property>
		<property name="classId" type="java.lang.String">
			<column name="CLASS_ID" length="33" />
		</property>
		<property name="serverAddress" type="java.lang.String">
			<column name="SERVER_ADDRESS" length="200" />
		</property>
		<property name="memo" type="java.lang.String">
			<column name="SERVICE_MEMO" length="200" />
		</property>
		<property name="className" type="java.lang.String">
			<column name="CLASS_NAME" length="200" />
		</property>
		<property name="codeType" type="java.lang.String">
			<column name="CODE_TYPE" length="10" />
		</property>
		<property name="userClassName" type="java.lang.String">
			<column name="USER_CLASS_NAME" length="200" />
		</property>
		
	</class>
</hibernate-mapping>
