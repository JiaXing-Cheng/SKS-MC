<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.more.fw.webservice.model.ClientModuleDto" table="MS_WS_CLIENT_MODEL">
		<id name="id" type="java.lang.String">
			<column name="ID" length="33" />
		</id>
		<property name="deptId" type="java.lang.String">
			<column name="DEPT_ID" length="33" />
		</property>
		<property name="createUserId" type="java.lang.String">
			<column name="CREATE_USER" length="33" />
		</property>
		<property name="createTime" type="java.util.Date">
			<column name="CREATE_TIME" />
		</property>
		<property name="editUserId" type="java.lang.String">
			<column name="EDIT_USER" length="33" />
		</property>
		<property name="editTime" type="java.util.Date">
			<column name="EDIT_TIME"/>
		</property>
		<property name="name" type="java.lang.String">
			<column name="MODEL_NAME" length="50" />
		</property>
		<property name="memo" type="java.lang.String">
			<column name="MODEL_MEMO" length="100" />
		</property>
		<property name="upId" type="java.lang.String">
			<column name="UP_ID" length="33" />
		</property>
	</class>
</hibernate-mapping>
