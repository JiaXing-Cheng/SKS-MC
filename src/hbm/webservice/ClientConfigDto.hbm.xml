<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.more.fw.webservice.model.ClientConfigDto" table="MS_WS_CLIENT">
		<id name="id" type="java.lang.String">
			<column name="ID" length="33" />
		</id>
		<property name="deptId" type="java.lang.String">
			<column name="DEPT_ID" length="33" />
		</property>
		<property name="createUserId" type="java.lang.String">
			<column name="CREATE_USER" length="33" />
		</property>
		<property name="createTime" type="java.util.Date">
			<column name="CREATE_TIME" />
		</property>
		<property name="editUserId" type="java.lang.String">
			<column name="EDIT_USER" length="33" />
		</property>
		<property name="editTime" type="java.util.Date">
			<column name="EDIT_TIME"/>
		</property>
		<property name="name" type="java.lang.String">
			<column name="SERVICE_NAME" length="50" />
		</property>
		<property name="type" type="java.lang.String">
			<column name="SERVICE_TYPE" length="1" />
		</property>
		<property name="moduleId" type="java.lang.String">
			<column name="MODEL_ID" length="33" />
		</property>
		<property name="classId" type="java.lang.String">
			<column name="CLASS_ID" length="33" />
		</property>
		<property name="serverAddress" type="java.lang.String">
			<column name="SERVER_ADDRESS" length="200" />
		</property>
		<property name="memo" type="java.lang.String">
			<column name="SERVICE_MEMO" length="200" />
		</property>
		<property name="packagePath" type="java.lang.String">
			<column name="PACKAGE_PATH" length="200" />
		</property>
		<property name="classFileName" type="java.lang.String">
			<column name="CLASS_FILE_NAME" length="2000" />
		</property>
		
	</class>
</hibernate-mapping>
